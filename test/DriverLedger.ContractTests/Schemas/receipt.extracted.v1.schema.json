{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "receipt.extracted.v1",
    "type": "object",
    "required": [ "receiptId", "fileObjectId", "confidence", "isHold", "holdReason" ],
    "properties": {
        "receiptId": {
            "type": "string",
            "format": "uuid"
        },
        "fileObjectId": {
            "type": "string",
            "format": "uuid"
        },
        "confidence": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
        },
        "isHold": { "type": "boolean" },
        "holdReason": { "type": "string" }
    },
    "additionalProperties": false
}
