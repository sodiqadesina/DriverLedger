{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "statement.parsed.v1",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "StatementId",
        "TenantId",
        "Provider",
        "PeriodType",
        "PeriodKey",
        "LineCount",
        "IncomeTotal",
        "FeeTotal",
        "TaxTotal"
    ],
    "properties": {
        "StatementId": {
            "type": "string",
            "format": "uuid"
        },
        "TenantId": {
            "type": "string",
            "format": "uuid"
        },
        "Provider": { "type": "string" },
        "PeriodType": { "type": "string" },
        "PeriodKey": { "type": "string" },
        "LineCount": {
            "type": "integer",
            "minimum": 0
        },
        "IncomeTotal": { "type": "number" },
        "FeeTotal": { "type": "number" },
        "TaxTotal": { "type": "number" }
    }
}
